# Staging Environment Variables
HEADAI_ENV=staging

# Database
DB_HOST=staging-db.headai.com
DB_PORT=5432
DB_USER=${STAGING_DB_USER}
DB_PASSWORD=${STAGING_DB_PASSWORD}

# Redis
REDIS_HOST=staging-redis.headai.com
REDIS_PORT=6379
REDIS_PASSWORD=${STAGING_REDIS_PASSWORD}

# API
API_SECRET_KEY=${STAGING_API_SECRET}
JWT_SECRET=${STAGING_JWT_SECRET}

# AI Models
OPENAI_API_KEY=${STAGING_OPENAI_API_KEY}

# Storage
AWS_ACCESS_KEY_ID=${STAGING_AWS_ACCESS_KEY}
AWS_SECRET_ACCESS_KEY=${STAGING_AWS_SECRET_KEY}
AWS_DEFAULT_REGION=us-east-1

# Monitoring
SENTRY_DSN=${STAGING_SENTRY_DSN}
ANALYTICS_ID=${STAGING_ANALYTICS_ID}

# Email
SMTP_HOST=smtp.staging.headai.com
SMTP_PORT=587
SMTP_USER=${STAGING_SMTP_USER}
SMTP_PASSWORD=${STAGING_SMTP_PASSWORD}

# Slack
SLACK_WEBHOOK_URL=${STAGING_SLACK_WEBHOOK}

# Feature Flags
ENABLE_EXPERIMENTAL=true
ENABLE_BETA_FEATURES=true
